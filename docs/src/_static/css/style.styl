.wy-nav-content
    max-width: 950px !important

.table
    width: 100%
    margin-top: 10px
    display: flex
    flex-direction: column

.tableheader
    display: flex
    flex-direction: row
    font-weight: bold
    justify-content: space-between
    align-items: flex-start
    align-content: flex-start

.tablerow
    display: flex
    flex-direction: row
    justify-content: space-between
    align-items: flex-start
    align-content: flex-start
    height: 170px
    margin-bottom: 10px

#resultimg
    object-fit: contain
    width: 170px
    height: 170px

.tablerow > p, .tableheader > p
    flex-grow: 1;
    flex-basis: 0;

.tablerow > img
    flex-grow: 1;
    flex-basis: 0;
